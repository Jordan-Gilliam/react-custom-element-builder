{
  "name": "react-custom-element-builder",
  "version": "1.0.0",
  "description": "Builds custom elements from React components",
  "main": "index.esm.js",
  "scripts": {
    "build": "npx babel index.js --out-file index.esm.js",
    "test": "npm run lint && jest --collect-coverage",
    "lint": "eslint index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jherr/react-custom-element-builder"
  },
  "author": "Jack Herrington <jherr@pobox.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-shadow-dom-retarget-events": "^1.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js"
    ],
    "testMatch": [
      "**/*.spec.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
